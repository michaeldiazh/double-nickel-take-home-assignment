[
  {
    "testName": "building basic select query for address",
    "input": {
      "selectColumns": ["id", "city", "state", "zip_code"],
      "fromTable": "address"
    },
    "expected": {
      "query": "select id, city, state, zip_code\n    from address",
      "values": []
    }
  },
  {
    "testName": "building select query for address with WHERE clause",
    "input": {
      "selectColumns": ["id", "city", "state"],
      "fromTable": "address",
      "whereClause": [
        {
          "tableName": "address",
          "column": "state",
          "filter": "=",
          "value": "CA"
        }
      ]
    },
    "expected": {
      "query": "select id, city, state\n    from address\n    \n    where address.state = $1",
      "values": ["CA"]
    }
  },
  {
    "testName": "building select query for address with multiple WHERE conditions",
    "input": {
      "selectColumns": ["id", "city", "state", "zip_code"],
      "fromTable": "address",
      "whereClause": [
        {
          "tableName": "address",
          "column": "state",
          "filter": "in",
          "value": ["CA", "NY", "TX"]
        },
        {
          "tableName": "address",
          "column": "city",
          "filter": "like",
          "value": "San%"
        }
      ]
    },
    "expected": {
      "query": "select id, city, state, zip_code\n    from address\n    \n    where address.state in ($1, $2, $3) and address.city like $4",
      "values": ["CA", "NY", "TX", "San%"]
    }
  },
  {
    "testName": "building select query for address with ORDER BY multiple columns",
    "input": {
      "selectColumns": ["id", "city", "state", "zip_code"],
      "fromTable": "address",
      "orderBy": [
        {
          "tableName": "address",
          "column": "state",
          "orderOption": "asc"
        },
        {
          "tableName": "address",
          "column": "city",
          "orderOption": "asc"
        }
      ]
    },
    "expected": {
      "query": "select id, city, state, zip_code\n    from address\n    \n    \n    \n    order by address.state asc, address.city asc",
      "values": []
    }
  },
  {
    "testName": "building select query for address with WHERE, ORDER BY, LIMIT, and OFFSET",
    "input": {
      "selectColumns": ["id", "city", "state"],
      "fromTable": "address",
      "whereClause": [
        {
          "tableName": "address",
          "column": "state",
          "filter": "=",
          "value": "NY"
        }
      ],
      "orderBy": [
        {
          "tableName": "address",
          "column": "city",
          "orderOption": "desc"
        }
      ],
      "limit": 25,
      "offset": 10
    },
    "expected": {
      "query": "select id, city, state\n    from address\n    \n    where address.state = $1\n    \n    order by address.city desc\n    limit 25\n    offset 10",
      "values": ["NY"]
    }
  },
  {
    "testName": "building select query for address with WHERE composition (OR)",
    "input": {
      "selectColumns": ["id", "city", "state"],
      "fromTable": "address",
      "whereClause": {
        "compositionOption": "or",
        "whereOptions": [
          {
            "tableName": "address",
            "column": "city",
            "filter": "=",
            "value": "New York"
          },
          {
            "tableName": "address",
            "column": "city",
            "filter": "=",
            "value": "Los Angeles"
          }
        ]
      }
    },
    "expected": {
      "query": "select id, city, state\n    from address\n    \n    (address.city = $1 or address.city = $2)",
      "values": ["New York", "Los Angeles"]
    }
  },
  {
    "testName": "building select query for address with date range WHERE clause",
    "input": {
      "selectColumns": ["id", "city", "state", "created_at"],
      "fromTable": "address",
      "whereClause": [
        {
          "tableName": "address",
          "column": "created_at",
          "filter": ">=",
          "value": "2024-01-01T00:00:00.000Z"
        },
        {
          "tableName": "address",
          "column": "created_at",
          "filter": "<=",
          "value": "2024-12-31T00:00:00.000Z"
        }
      ],
      "orderBy": [
        {
          "tableName": "address",
          "column": "created_at",
          "orderOption": "desc"
        }
      ]
    },
    "expected": {
      "query": "select id, city, state, created_at\n    from address\n    \n    where address.created_at >= $1 and address.created_at <= $2\n    \n    order by address.created_at desc",
      "values": ["2024-01-01T00:00:00.000Z", "2024-12-31T00:00:00.000Z"]
    }
  }
]

